{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.6.0" },
        "public/index.php": { "runtime": "vercel-php@0.6.0" }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://${VERCEL_URL}",
        "CONFIG_CACHE_PATH": "/tmp/config.php",
        "ROUTES_CACHE_PATH": "/tmp/routes.php",
        "VIEW_COMPILED_PATH": "/tmp",
        "SESSION_DRIVER": "cookie",
        "LOG_CHANNEL": "stderr"
    }
}